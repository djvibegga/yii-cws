<server name="testc" host="127.0.0.1" port="9015">
    <requestConcurrency value="1000"/>
    <instance>
        <count value="1"/>
        <idleTimeout value="100000"/>
    </instance>
    <components>
        <db>
            <cdn value="mysql:host=127.0.0.1;port=3306;dbname=test"/>
            <username value="root"/>
            <password value=""/>
        </db>
        <urlManager>
            <urlFormat value="path"/>
        </urlManager>
        <session>
            <autoOpen value="true" />
            <gcTimeout value="6" />
        </session>
        <clientScript>
            <packages>
                <package name="subtest">
                    <basePath value="application.assets" />
                    <js>
                        <file path="subassets/sub.js" />
                    </js>
                    <depends>
                        <package name="jquery" />
                    </depends>
                </package>
                <package name="test">
                    <basePath value="application.assets" />
                    <js>
                        <file path="test.js" />
                    </js>
                    <css>
                        <file path="style.css" />
                    </css>
                    <depends>
                        <package name="jquery" />
                    </depends>
                </package>
            </packages>
        </clientScript>
    </components>
</server>
